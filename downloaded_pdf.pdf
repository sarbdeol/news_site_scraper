  const embedCode = 'https://www.youtube.com/embed/'+embedCodeID+'?autoplay=1'
 
   console.log(embedCode);
  // Create the YouTube embed element.
  iframe.src = embedCode;
  const iframeWrap = element.querySelector(".embed-container"); 
  iframeWrap.style.display = "block"; 

}

// Attach the event listener to all elements with the class `.yt-embed`.
